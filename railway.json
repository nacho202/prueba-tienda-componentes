{
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "npm start",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  },
  "routes": {
    "/": "index.html",
    "/admin": "admin.html",
    "/producto": "producto.html",
    "/carrito": "carrito.html",
    "/checkout": "checkout.html"
  }
}
